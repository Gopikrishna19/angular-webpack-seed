{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "consistent-return": 2,
    "complexity": [
      2,
      3
    ],
    "arrow-spacing": 2,
    "curly": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [
      2,
      "smart"
    ],
    "no-alert": 2,
    "no-caller": 2,
    "no-console": 2,
    "no-else-return": 2,
    "no-magic-numbers": 2,
    "no-multi-spaces": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-var": 2,
    "object-shorthand": 2,
    "one-var": [
      2,
      "never"
    ],
    "prefer-const": 2,
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "no-invalid-this": 2,
    "vars-on-top": 2,
    "yoda": 2
  }
}
